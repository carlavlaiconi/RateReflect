{
  "_version": "1.49.0",
  "sap.app": {
    "id": "ratereflect",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.10.5",
      "toolsId": "fb80c015-dec5-4c52-b179-d07ef751ec74"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/Z_RATEREFLECT_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.117.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ratereflect.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "metadataUrlParams": {
            "sap-documentation": "heading"
          },
          "operationMode" : "Server",
          "synchronizationMode" : "None",
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "useBatch": false,
          "defaultUpdateMethod": "PUT"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "ratereflect.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": ["TargetView1"]
        },
        {
          "name": "Feedback",
          "pattern": "/Feedback",
          "target":"Feedback"
        },
        {
          "name": "SendFeedback360",
          "pattern": "/SendFeedback360",
          "target":"SendFeedback360"
        },
        {
          "name": "UserStart",
          "pattern": "/UserStart",
          "target": "UserStart"
        },
        {
        "name": "UserInfo",
        "pattern": "/UserInfo",
        "target": "UserInfo"
        }
        

      ],
      "targets": {
        "TargetView1": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "View1",
          "viewName": "View1"
        },
        "UserStart": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UserStart",
          "viewName": "UserStart"
        },
        "SendFeedback360": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "SendFeedback360",
          "viewName": "SendFeedback360"
        },
        "UserInfo": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UserInfo",
          "viewName": "UserInfo"
        },
        "Feedback": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Feedback",
          "viewName": "Feedback"
        }
      }
    },
    "rootView": {
      "viewName": "ratereflect.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
