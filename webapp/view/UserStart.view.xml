<mvc:View
	  controllerName="ratereflect.controller.UserStart"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    
	<SplitApp
		id="SplitAppDemo"
		initialDetail="detail"
		initialMaster="master">
		<detailPages>
			 <Page
				id="detail"
				title="Feedbacks"
				backgroundDesign= "Solid"> 
        
			</Page>
			<Page
				id="detailDetail"
				title="Send 360 feedback"
				backgroundDesign= "Solid"
				showNavButton="false"
				navButtonPress=".onPressDetailBack">
				
			</Page>
			<Page
				id="detail2"
				title="Request PEG"
				backgroundDesign= "Solid"
				showNavButton="false"
				navButtonPress=".onPressDetailBack">

			</Page>
		</detailPages>
		<masterPages>
			<!-- <Page
				id="master"
				title="Master 1"
				backgroundDesign= "List">
				<List id="_IDGenList1">
					<StandardListItem id="_IDGenStandardListItem1" title="To Master2" type="Navigation" press=".onPressGoToMaster" />
				</List>
			</Page> -->
			<Page
				id="master2"
				title="Hello, User!"
				backgroundDesign="List"
				showNavButton="false"
				navButtonPress=".onPressMasterBack" >
        <VBox id="_IDGenVBox2">
           <!-- /<Button id="_IDGenbutton" visible="true" class="orice"/>		 -->
        </VBox>
				<List id="_IDGenList2" itemPress=".onListItemPress">
					<items>
          	<StandardListItem id="_IDGenStandardListItem2" title="Feedbacks" type="Active" custom:to="detail" />
						<StandardListItem id="_IDGenStandardListItem3" title="Send 360 feedback" type="Active" custom:to="detailDetail" />
						<StandardListItem id="_IDGenStandardListItem4" title="Request PEG" type="Active" custom:to="detail2" />
					</items>
        
				</List>
			</Page>
		</masterPages>
	</SplitApp>
</mvc:View>
