<mvc:View
    controllerName="ratereflect.controller.UserStart"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
           <Table id="idProductsTable"
        inset="false"
        items="{
          path: '/Users'
        }">
        <headerToolbar>
          <Toolbar id="_IDGenToolbar1">
            <Label id="_IDGenLabel1" text="Users"></Label>
          </Toolbar>
        </headerToolbar>
        <columns>
          <Column id="_IDGenColumn1"
            width="12em">
            <Label id="_IDGenLabel2" text="Name" />
          </Column>
        </columns>
        <items>
          <ColumnListItem id="_IDGenColumnListItem1">
            <cells>
              <Text id="idListItem" text="{Email}" />
            </cells>
          </ColumnListItem>
        </items>
</Table>
        </content>
    </Page>
</mvc:View>
